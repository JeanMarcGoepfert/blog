# A sample Gemfile
source "https://rubygems.org"

group :jekyll do
    gem 'jekyll'
    gem 'jekyll-sitemap'
    gem 'jekyll-redirect-from'
    gem 'jekyll-picture-tag'
    gem 'jekyll-archives'
end

group :development do
    gem 'rouge'
    gem 'mini_magick'
    gem 'fileutils'
    gem 'gsl'
    gem 'narray'
end
