# A sample Gemfile
source "https://rubygems.org"

group :jekyll do
    gem 'jekyll'
    gem 'jekyll-sitemap'
    gem 'jekyll-redirect-from'
    gem 'jekyll-picture-tag'
    gem 'jekyll-archives'
end

group :development do
    gem 'classifier-reborn', :git => 'https://github.com/jekyll/classifier-reborn.git'
    gem 'rouge', '1.9.0'
    gem 'mini_magick'
    gem 'fileutils'
    gem 'gsl'
    gem 'rb-gsl'
    gem 'narray'
end
