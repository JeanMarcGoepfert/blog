    "lint:js": "eslint ./gatsby-*.js && eslint ./src/**/*.jsx",
    "lint:css": "stylelint ./src/**/*.{css,scss}",
    "lint": "npm run svg && npm run lint:js && npm run lint:css",
    "test": "npm run lint",
    "deploy": "./scripts/deploy.sh",
    "svg": "./scripts/svg.sh"
  },
  "browserslist": [
    "last 3 versions"
  ],
  "dependencies": {
    "clipboard": "^2.0.1",
    "exif-js": "^2.3.0",
