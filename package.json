    "node-sass": "^4.9.3",
    "nord": "^0.2.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.0",
    "react-clipboard.js": "^2.0.0",
    "react-dom": "^16.5.0",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.5.1",
    "react-qr-svg": "^2.1.0",
